<h1>New resource</h1>

<% form_for @resource, :html => {:multipart => true} do |f| %>
<%= f.error_messages %>
<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
  </p>
<p>
    <%= f.label :file %><br />
    <%= f.file_field :file %>
</p>
<p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
</p>
<p>
  <%= f.submit 'Create' %>
</p>
<% end %>
